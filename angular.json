            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/maca-join-pipe/src/test.ts",
            "tsConfig": "projects/maca-join-pipe/tsconfig.spec.json",
            "karmaConfig": "projects/maca-join-pipe/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/maca-join-pipe/tsconfig.lib.json",
              "projects/maca-join-pipe/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "maca-spinner": {
      "projectType": "library",
      "root": "projects/maca-spinner",
      "sourceRoot": "projects/maca-spinner/src",
      "prefix": "maca",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/maca-spinner/tsconfig.lib.json",
            "project": "projects/maca-spinner/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/maca-spinner/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/maca-spinner/src/test.ts",
            "tsConfig": "projects/maca-spinner/tsconfig.spec.json",
            "karmaConfig": "projects/maca-spinner/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/maca-spinner/tsconfig.lib.json",
              "projects/maca-spinner/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "maca-toast": {
      "projectType": "library",
      "root": "projects/maca-toast",
      "sourceRoot": "projects/maca-toast/src",
      "prefix": "maca",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/maca-toast/tsconfig.lib.json",
            "project": "projects/maca-toast/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/maca-toast/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/maca-toast/src/test.ts",
            "tsConfig": "projects/maca-toast/tsconfig.spec.json",
            "karmaConfig": "projects/maca-toast/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/maca-toast/tsconfig.lib.json",
              "projects/maca-toast/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-maca",
  "cli": {
    "analytics": false
  }
}